package com.chainsys.testtry;

public class sum {

	public static void main(String[] args) {
	sums();
	sub();

	}
public static void sums () 
{
    int a=7;
    int b=8;
    int c=a+b;
    System.out.println(c);
}
public static void sub () 
{
    int a=7;
    int b=8;
    int c=a-b;
    System.out.println(c);
}
}
